<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
    version="1.0">

    <persistence-unit name="DEFAULTJPA">
        <!-- Add all of your model classes here -->
        <class>Tests.Test</class>
        <class>WekaDataTables.Homosapiens_Gene_Info</class>
        <class>org.scripps.branch.entity.FeatureDB</class>
        <class>org.scripps.branch.entity.AttributeDB</class>
        <class>org.scripps.branch.entity.TreeDB</class>
        <class>org.scripps.branch.entity.User</class>
        <class>org.scripps.branch.service.FeatureBuilder</class>
        <class>org.scripps.branch.service.AttributeBuilder</class>
        
        
        
        
        <exclude-unlisted-classes />
        <properties>
            <!-- Update these datastore details if different -->
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/branch"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.user" value="postgres"/>
            <property name="javax.persistence.jdbc.password" value="prime"/>

            
        </properties>
    </persistence-unit>

</persistence>




